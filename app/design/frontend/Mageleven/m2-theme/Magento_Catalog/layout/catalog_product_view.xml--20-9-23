<page layout="2columns-left" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="catalog.compare.sidebar" remove="true"/>
        <referenceBlock name="wishlist_sidebar" remove="true"/>
        <referenceBlock name="breadcrumbs" remove="true" />
        <referenceContainer name="main.content" htmlClass="extdetail" htmlId="extdetail"/>
        <referenceContainer name="columns" htmlClass="container" />
        <referenceContainer name="main" htmlClass="row" />
        <referenceContainer name="main">
            <container name="leftpart" htmlTag="div" htmlClass="col-lg-12" before="-"/>
            <container name="rightpart" htmlTag="div" htmlClass="col-lg-4" after="leftpart"/>
            <container name="leftpartinrow" htmlTag="div" htmlClass="row"/>
            <container name="rightpartinrow" htmlTag="div" htmlClass="edition"/>
        </referenceContainer>
        <referenceContainer name="product.info.main" htmlTag="div" htmlClass="product-info-main col-md-6" />
        <referenceContainer name="product.info.media" htmlTag="div" htmlClass="product media col-md-6" />

        <move element="leftpartinrow" destination="leftpart" before="-"/>
        <move element="product.info.main" destination="leftpartinrow" after="product.info.media"/>
        <move element="product.info.media" destination="leftpartinrow" before="-"/>
        <move element="product.info.details" destination="leftpartinrow" after="product.info.main"/>
        <move element="require-cookie" destination="leftpartinrow" after="product.info.details"/>
        <move element="rightpartinrow" destination="rightpart" before="-"/>
        <move element="product.info" destination="rightpartinrow" before="-"/>
       

        <referenceContainer name="page.wrapper">
            <container name="otherproductsection" htmlTag="section" htmlClass="newextension m2ext" after="main.content"/>
        </referenceContainer>
        <move element="otherproductsection" destination="main.content" after="-"/>
        <move element="catalog.product.related" destination="otherproductsection" before="-"/>
        <move element="product.info.upsell" destination="otherproductsection" after="catalog.product.related"/>
        <move element="product.info.additional" destination="otherproductsection" after="product.info.upsell"/>
        

        <move element="product.price.final" destination="product.info.options.wrapper.bottom" before="-"/>
        <referenceBlock name="product.info.price" remove="true" />
        <referenceBlock name="product.info.social" remove="true" />

       <!--  <block class="Magento\Framework\View\Element\Template" name="pagespeed" template="Magento_Catalog::product/view/pagespeed.phtml"></block>
        <move element="pagespeed" destination="product.info.main" after="-"/> -->
        
    </body>
</page>
