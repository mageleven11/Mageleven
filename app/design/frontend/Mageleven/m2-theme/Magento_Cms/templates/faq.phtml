<script>
require(['jquery'],function($){
  $(document).ready(function(){
    $('.question_type').click(function(){
      // Toggle 'active' class for the clicked question_type
      $(this).toggleClass("active");

      // Remove 'active' class from all other question_type elements
      $('.question_type').not(this).removeClass('active');

      // Toggle display for show_question
      var showQuestion = $(this).next('.show_question');
      $('.show_question').not(showQuestion).slideUp();

      // Trigger click event on the first question's toggle immediately
      // showQuestion.find('.inner_question:first .toggle').trigger('click');

      showQuestion.slideToggle();
    });

    // Updated click event handler for inner_question (Highlighted from here)
    $(document).on('click', '.inner_question', function() {
      $(this).toggleClass("active");
      $('.inner_question').not(this).removeClass('active');
      var answer = $(this).next('.inner_answer');
      $('.inner_answer').not(answer).slideUp();
      answer.slideToggle();
    }); // (Highlighted to here)

    // Hide all answers initially
    $('.inner_answer').hide();
  });
});
</script>


<script>
        require(['jquery'],function($){
         $(document).ready(function () {
            $('#searchInput').on('input', function () {
                var keywords = $(this).val().toLowerCase().trim();
                var questions = $('.inner_question');
                var answers = $('.inner_answer');
                var matchedQuestionsContainer = $('#matchedQuestions');

                // Clear existing search results and hide all questions initially
                matchedQuestionsContainer.empty();
                $('.all_question').hide().removeClass('iamwalker'); // Hide and remove iamwalker class from all_question divs
                $('.question_type').removeClass('lockmenow iamwalker'); // Remove lockmenow and iamwalker class from all question_type divs

                // If input is empty, show all questions and exit the function
                if (keywords === '') {
                    $('.all_question').show();
                    questions.removeClass('matched'); // Remove matched class from all questions
                    matchedQuestionsContainer.hide(); // Hide matchedQuestionsContainer if there are no search keywords
                    return;
                }

                questions.each(function (index) {
                    var questionContainer = $(this).closest('.question');
                    var questionHtml = questionContainer.closest('.all_question');



                    var questionText = $(this).text().toLowerCase();
                    var answerText = answers.eq(index).text().toLowerCase(); // Get corresponding answer text

                    // Check if question or answer contains keywords
                    if (questionText.includes(keywords) || answerText.includes(keywords)) {
                        // Show the parent all_question div if it contains a matched question
                        questionHtml.show().addClass('iamwalker'); // Add iamwalker class to all_question

                        // Activate the corresponding question_type and add iamwalker class
                        var questionType = questionHtml.prev('.question_type');
                        questionType.addClass('lockmenow iamwalker');
                        questionType.find('.toggle').trigger('click'); // Ensure toggle functionality works

                        // Clone the matched question and append it to the matchedQuestionsContainer
                        questionContainer.clone().appendTo(matchedQuestionsContainer);
                        questionContainer.addClass('matched');
                        if (questionContainer.hasClass('matched')) {
                            questionContainer.find('.inner_answer').removeAttr('style');
                        }
                    } else {
                        questionContainer.removeClass('matched');
                    }
                });

                // $('.all_question.iamwalker:visible').each(function () {
                //     var questionType = $(this).prev('.question_type');
                //     questionType.addClass('lockmenow iamwalker');
                //     questionType.find('.toggle').trigger('click'); // Ensure toggle functionality works
                // });

                // Show matchedQuestionsContainer only if no matches are found
                if ($('#matchedQuestions').children().length === 0) {
                    matchedQuestionsContainer.show();
                    $("#matchedQuestions").html('<div class="matchedQuestions_image"></div><strong><span>No Results Found</span> Please try another keyword to find relevant queries.</strong>');
                    $(".faq-list-page").addClass("matchedQuestions_error");
                } else {
                    matchedQuestionsContainer.hide();
                    $(".faq-list-page").removeClass("matchedQuestions_error");
                }
            });
        });
        });
    </script>


