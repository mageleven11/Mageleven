<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="mageleven-script-src" type="host">www.mageleven.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="mageleven-connect-src" type="host">www.mageleven.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="mageleven-img-src" type="host">www.mageleven.com</value>
            </values>
        </policy>
        <policy id="media-src">
            <values>
                <value id="mageleven-media-src" type="host">www.mageleven.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
